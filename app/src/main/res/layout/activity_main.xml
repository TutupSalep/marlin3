<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/linearLayout2"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical"
  tools:context="com.rakasettya.marlin3.apps.main.MainActivity">

  <Button
    android:id="@+id/btnTest2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="44dp"
    android:text="@string/test_2"
    app:layout_constraintEnd_toEndOf="parent"
    tools:ignore="MissingConstraints"
    tools:layout_conversion_absoluteHeight="48dp"
    tools:layout_conversion_absoluteWidth="192dp"
    tools:layout_editor_absoluteY="247dp"/>
  <Button
    android:id="@+id/btnTest1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="68dp"
    android:text="@string/test_1"
    app:layout_constraintEnd_toStartOf="@+id/btnTest2"
    app:layout_constraintStart_toStartOf="parent"
    tools:ignore="MissingConstraints"
    tools:layout_conversion_absoluteHeight="48dp"
    tools:layout_conversion_absoluteWidth="192dp"
    tools:layout_editor_absoluteY="247dp"/>
</android.support.constraint.ConstraintLayout>
